+
+	if (zend_parse_parameters(argc TSRMLS_CC, "|l", &flag) == FAILURE) {
+		return;
+	}
+
+	if(!argc) {
+		flag = PHP_CREDITS_ALL;
+	} 
+
+	php_print_credits(flag);
